# B3 AI Analysis Platform - Frontend Environment Variables

# API Backend URL
NEXT_PUBLIC_API_URL=http://localhost:3101/api/v1

# WebSocket URL
NEXT_PUBLIC_WS_URL=http://localhost:3101

# OAuth API Service URL (FastAPI service on port 8000)
NEXT_PUBLIC_OAUTH_URL=http://localhost:8000

# App Environment
NEXT_PUBLIC_ENV=development

# Google OAuth (opcional)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=

# Analytics (opcional)
NEXT_PUBLIC_GA_ID=

# Feature Flags
NEXT_PUBLIC_ENABLE_AI_ANALYSIS=true
NEXT_PUBLIC_ENABLE_REALTIME_DATA=true
NEXT_PUBLIC_ENABLE_SOCIAL_LOGIN=true
